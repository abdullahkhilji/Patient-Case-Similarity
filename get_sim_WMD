    "def df_to_one_hot(posn):\n",
    "    a2 = pd.get_dummies(df.T.iloc[posn])\n",
    "    a3 = a2.values.sum(axis=1)\n",
    "    a3 = a2.join('')\n",
    "    a3 = pd.Series(a2.fillna('').values.tolist()).str.join('')\n",
    "    a2 = a2.applymap(str)\n",
    "    a = a2.fillna('').values.tolist()\n",
    "    a = pd.Series(a)\n",
    "    a = a.str.join('')\n",
    "    return a\n",
    "\n",
    "    \n",
    "    \n",
    "    "
   ]
